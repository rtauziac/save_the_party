script/source = "extends ColorRect


# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	fade_in()


func fade_in():
	var fade_in_tween = get_tree().create_tween()
	fade_in_tween.tween_property(self, \"color\", Color.TRANSPARENT, 1)


func fade_out():
	var fade_in_tween = get_tree().create_tween()
	fade_in_tween.tween_property(self, \"color\", Color.TRANSPARENT, 1)
"

